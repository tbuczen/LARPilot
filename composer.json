{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.2",
        "ext-ctype": "*",
        "ext-dom": "*",
        "ext-iconv": "*",
        "ext-libxml": "*",
        "doctrine/dbal": "^3.10.3",
        "doctrine/doctrine-bundle": "^2.18.1",
        "doctrine/doctrine-migrations-bundle": "^3.7",
        "doctrine/orm": "^3.5.7",
        "excelwebzone/recaptcha-bundle": "^1.5.42",
        "ezyang/htmlpurifier": "^4.19",
        "google/apiclient": "^2.18.4",
        "knplabs/knp-paginator-bundle": "^6.9.1",
        "knpuniversity/oauth2-client-bundle": "^2.19",
        "league/oauth2-facebook": "^2.2",
        "league/oauth2-google": ">=4.0.1",
        "moneyphp/money": "^4.8",
        "nikic/php-parser": "^5.6.2",
        "phpdocumentor/reflection-docblock": "^5.6.3",
        "phpstan/phpdoc-parser": "^2.3",
        "scienta/doctrine-json-functions": "^6.3",
        "shipmonk/doctrine-entity-preloader": "^1.2",
        "spiriitlabs/form-filter-bundle": "^11.1.2",
        "stof/doctrine-extensions-bundle": "^1.14",
        "symfony/asset": "7.2.*",
        "symfony/asset-mapper": "7.2.*",
        "symfony/console": "7.2.*",
        "symfony/doctrine-messenger": "7.2.*",
        "symfony/dotenv": "7.2.*",
        "symfony/expression-language": "7.2.*",
        "symfony/flex": "^2.9",
        "symfony/form": "7.2.*",
        "symfony/framework-bundle": "7.2.*",
        "symfony/http-client": "7.2.*",
        "symfony/intl": "7.2.*",
        "symfony/mailer": "7.2.*",
        "symfony/mime": "7.2.*",
        "symfony/monolog-bundle": "^3.10",
        "symfony/notifier": "7.2.*",
        "symfony/process": "7.2.*",
        "symfony/property-access": "7.2.*",
        "symfony/property-info": "7.2.*",
        "symfony/runtime": "7.2.*",
        "symfony/security-bundle": "7.2.*",
        "symfony/serializer": "7.2.*",
        "symfony/stimulus-bundle": "^2.31",
        "symfony/string": "7.2.*",
        "symfony/translation": "7.2.*",
        "symfony/twig-bundle": "7.2.*",
        "symfony/uid": "7.2.*",
        "symfony/ux-autocomplete": "^2.31",
        "symfony/ux-live-component": "^2.31",
        "symfony/ux-twig-component": ">=2.31",
        "symfony/validator": "7.2.*",
        "symfony/web-link": "7.2.*",
        "symfony/workflow": "7.2.*",
        "symfony/yaml": "7.2.*",
        "symfonycasts/dynamic-forms": "^0.1.3",
        "symfonycasts/sass-bundle": "^0.8.3",
        "twig/extra-bundle": "^2.12|^3.22",
        "twig/string-extra": "^3.22",
        "twig/twig": "^2.12|^3.22",
        "wohali/oauth2-discord-new": ">=1.2.1",
        "yceruto/money-bundle": "^1.3.4"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "bump-after-update": true,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "importmap:install": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "pre-autoload-dump": "Google\\Task\\Composer::cleanup",
        "ecs": "vendor/bin/ecs check",
        "phpstan": "vendor/bin/phpstan analyse -c phpstan.neon"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "7.2.*"
        },
        "google/apiclient-services": [
            "Drive",
            "Sheets",
            "Docs"
        ]
    },
    "require-dev": {
        "codeception/codeception": "^5.3.2",
        "codeception/module-asserts": "^3.2.1",
        "codeception/module-doctrine": "*",
        "codeception/module-phpbrowser": ">=3.0.2",
        "codeception/module-symfony": "^3.7.1",
        "codeception/module-webdriver": "^4.0.3",
        "doctrine/doctrine-fixtures-bundle": "^4.3",
        "phpstan/phpstan": "^2.1.32",
        "rector/rector": "^2.2.8",
        "symfony/browser-kit": "7.2.*",
        "symfony/css-selector": "7.2.*",
        "symfony/debug-bundle": "7.2.*",
        "symfony/maker-bundle": "^1.64",
        "symfony/stopwatch": "7.2.*",
        "symfony/web-profiler-bundle": "7.2.*",
        "symplify/easy-coding-standard": "^12.6.2",
        "zenstruck/foundry": "^2.8"
    }
}
